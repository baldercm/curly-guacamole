{

  "logger": {
    "main": {
      "console": {
        "level": "info",
        "json": true,
        "stringify": true
      },
      "file": {
        "level"   : "info",
        "json": true,
        "filename": "curly-guacamole.log"
      }
    },
    "access": {
      "console": {
        "level": "info",
        "json": true,
        "stringify": true
      },
      "file": {
        "level"   : "info",
        "json": true,
        "filename": "curly-guacamole-access.log"
      }
    }
  },

  "middleware": {
    "logger": {
      "priority": 50,
      "module": {
        "name": "path:./src/lib/logger",
        "method": "accessLogger"
      }
    },

    "router": {
      "priority": 120,
      "module": {
        "arguments": [{ "directory": "path:./src/controllers" }]
      }
    },

    "error": {
      "priority": 121,
      "module": {
        "name": "path:./src/middlewares/error",
        "method": "default"
      }
    },

    "favicon": {
      "enabled": false
    },

    "static": {
      "enabled": false
    },

    "urlencoded": {
      "enabled": false
    },

    "multipart": {
      "enabled": false
    },

    "cookieParser": {
      "enabled": false
    },

    "session": {
      "enabled": false
    },

    "appsec": {
        "enabled": false
    }
  }
}
