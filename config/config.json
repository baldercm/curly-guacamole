{

  "middleware": {
    "router": {
      "priority": 120,
      "module": {
        "arguments": [{ "directory": "path:./src/controllers" }]
      }
    },

    "error": {
      "priority": 121,
      "module": {
        "name": "path:./src/middlewares/error",
        "method": "default"
      }
    },

    "favicon": {
      "enabled": false
    },

    "static": {
      "enabled": false
    },

    "urlencoded": {
      "enabled": false
    },

    "multipart": {
      "enabled": false
    },

    "cookieParser": {
      "enabled": false
    },

    "session": {
      "enabled": false
    },

    "appsec": {
        "enabled": false
    }
  }
}
